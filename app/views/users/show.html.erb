<head>
  <title>Màn Hình Show</title>
</head>
<body>
  <h1>Màn Hình Show</h1>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">ID</th>
        <th scope="col">Email</th>
        <th scope="col">Name</th>
        <th scope="col">Phone</th>
        <th scope="col">Address</th>
      </tr>
    </thead>
    <tbody>
      <tr class="table-active">
        <td><%= @user.id %></td>
        <td><%= @user.email %></td>
        <td><%= @user.name %></td>
        <td><%= @user.phone %></td>
        <td><%= @user.address %></td>
      </tr>
    </tbody>
  </table>
</body>
